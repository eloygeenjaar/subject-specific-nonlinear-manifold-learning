{
    "run_type": "classification",
    "hyperparameter_range": {
        "num_layers": [0, 1, 2, 3],
        "hidden_dim": [32, 64, 128],
        "dropout": [0.0, 0.1],
        "seed": [42, 1337, 9999, 1212]
    },
    "trainer": {
        "epochs": 100,

        "save_dir": "saved/classification/",
        "verbosity": 2,
        "lambda_step": 0

    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.0001,
            "weight_decay": 0,
            "amsgrad": false
        }
    },
    "loss": "cross_entropy",
    "metrics": [
        "accuracy", "auroc", "auprc"
    ]
}